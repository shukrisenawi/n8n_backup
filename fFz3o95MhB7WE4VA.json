    {
      "parameters": {
        "jsCode": "const input = $('Start').first().json.text.trim();\n\nconst value = input.split(' ');\n\nfor (const item of $input.all()) {\n  item.json.value1 = value[0]||null;\n  if(value[1])\n    item.json.value2 = value[1].toUpperCase();\n  else\n    item.json.value2 = null;\n}\n\nreturn $input.all();"
      },
